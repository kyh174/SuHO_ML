    "# 데이터프레임 생성\n",
    "df = pd.DataFrame({'A':[1, 2, 3, 4], 'B':[5, 6, 7, 8], 'C':[9, 10, 11, 12]})\n",
    "print(df)\n",
    "print('\\n')\n",
    "\n",
    "# 특정 컬럼만 출력(선택)할 때\n",
    "# df['컬럼명']\n",
